  "shortcuts": [
    {
      "name": "Overview",
      "short_name": "Overview",
      "description": "View monthly overview and key metrics",
      "url": "/dashboard.html#overview",
      "icons": [
        {
          "src": "/icons/overview-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Reports",
      "short_name": "Reports",
      "description": "Upload and manage reports",
      "url": "/dashboard.html#reports",
      "icons": [
        {
          "src": "/icons/reports-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "View detailed analytics and charts",
      "url": "/dashboard.html#analytics",
      "icons": [
        {
          "src": "/icons/analytics-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Insights",
      "short_name": "Insights",
      "description": "View business insights and recommendations",
      "url": "/dashboard.html#insights",
      "icons": [
        {
          "src": "/icons/insights-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/overview-desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard Overview on Desktop"
    },
    {
      "src": "/screenshots/analytics-desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Analytics Section on Desktop"
    },
    {
      "src": "/screenshots/overview-mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Dashboard Overview on Mobile"
    },
    {
      "src": "/screenshots/analytics-mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Analytics Section on Mobile"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+fuel",
      "url": "/dashboard.html?protocol=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/dashboard.html",
      "accept": {
        "application/pdf": [".pdf"],
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/dashboard.html",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["application/pdf", "text/csv", "application/json"]
        }
      ]
    }
  }
}
